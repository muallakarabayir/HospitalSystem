@model IEnumerable<HospitalSystem.ViewModels.UsersViewModel>


@{
    ViewData["Title"] = "Home Page";
}
<a asp-controller="Account" asp-action="Login" class="btn btn-primary">Login</a>


@foreach(var user in Model)
{
    <h1>@user.IdentityNo</h1>
    <p>@user.Name</p>
    <p>@user.Surname</p>
    <p>@user.Phone</p>
    <p>@user.Email</p>
}